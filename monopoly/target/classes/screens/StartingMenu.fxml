    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Button text="Start Game" onAction="#handleStartSubmit"/>
    <Button text="Exit" onAction="#handleExitSubmit"/>
</VBox>